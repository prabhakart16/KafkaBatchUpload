{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Confluent.Kafka": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=BulkUploadDB;Integrated Security=true;TrustServerCertificate=true;MultipleActiveResultSets=true"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "ConsumerGroupId": "bulk-upload-consumer-group",
    "Topic": "bulk-upload-topic",
    "ConsumerConfig": {
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false,
      "EnableAutoOffsetStore": false,
      "MaxPollIntervalMs": 300000,
      "SessionTimeoutMs": 45000,
      "IsolationLevel": "ReadCommitted"
    }
  },
  "Processing": {
    "MaxRetryAttempts": 3,
    "BatchInsertSize": 1000,
    "RetryDelaySeconds": 2
  }
}