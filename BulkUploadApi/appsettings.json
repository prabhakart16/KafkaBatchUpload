{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Confluent.Kafka": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=BulkUploadDB;Integrated Security=true;TrustServerCertificate=true;MultipleActiveResultSets=true"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "bulk-upload-topic",
    "ProducerConfig": {
      "Acks": "All",
      "EnableIdempotence": true,
      "MaxInFlight": 5,
      "MessageSendMaxRetries": 10,
      "CompressionType": "Snappy",
      "LingerMs": 10,
      "BatchSize": 1000000,
      "RequestTimeoutMs": 30000
    }
  },
  "Upload": {
    "MaxChunkSize": 5000,
    "MaxKafkaMessageSize": 900000
  }
}